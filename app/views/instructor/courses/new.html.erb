<br />
<div class="booyah-box col-10 offset-1">
    <h1>Teach us your mad skills.</h1>
    <br />
    <%= simple_form_for @course, url: instructor_courses_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :image %>
    <br />
    <%= f.submit "Create", class: 'btn-btn-secondary' %>
    <br />
    <% end %>
    <br />
</div>